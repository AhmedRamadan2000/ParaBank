<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ParaBank">
    <parameter name="browser" value=""/>
    <test name="Check register with invalid data">
        <classes>
            <class name="testcases.TC01_Registration">
                <methods>
                    <include name="RegisterNewUserWithInvalidData_N"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Check login with valid username and Invalid password">
        <classes>
            <class name="testcases.TC02_LoginPage">
                <methods>
                    <include name="loginWithValidUsernameAndInvalidPassword_N"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Check login with invalid username and valid password">
        <classes>
            <class name="testcases.TC02_LoginPage">
                <methods>
                    <include name="loginWithInvalidUsernameAndValidPassword_N"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Check login with invalid username and invalid password">
        <classes>
            <class name="testcases.TC02_LoginPage">
                <methods>
                    <include name="loginWithInvalidUsernameAndInvalidPassword_N"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>